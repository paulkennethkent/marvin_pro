<%= error_messages_for(@ticket) %> 
<div class="control-group">
  <%= f.label :product_id, :class => 'control-label' %>
  <div class="controls">
    <%=  f.autocomplete_field :product, autocomplete_product_name_tickets_path, :id_element => '#ticket_product_id'  %>
  </div>
  <%= f.hidden_field :product_id %>
</div>
<div class="control-group">
  <%= f.label :status, :class => 'control-label' %>
  <div class="controls">
    <%= f.select(:status, ["Open", "Closed", "Paused"] ) %>
  </div>
</div>
<div class="control-group">
  <%= f.label :customer_id, :class => 'control-label' %>
  <div class="controls">
     <%= f.select(:customer_id, @customers.map {|c| [c.name, c.id]})  %>
  </div>
</div>
<div class="control-group">
  <%= f.label :school_id, :class => 'control-label' %>
  <div class="controls">
  <%= f.select(:school_id, @schools.map {|s| [s.name, s.id]})  %>
  </div>
</div>
<div class="control-group">
  <%= f.label :admin_user_id, :class => 'control-label' %>
  <div class="controls">
    <%= f.select(:admin_user_id, @admin_users.map {|s| [s.username, s.id]})  %>
  </div>
</div>