<% @page_title = "Delete Admin User: #{@admin_user.name}" %>
<div class="admins delete">
	<h2>Delete Admin User</h2>

	<%= form_for(:admin_user, :url => {:action => 'destroy', :id => @admin_user.id}) do |f| %>

	<p>Are you sure you want to permanently delete this subject?</p>
		<dl class="dl-horizontal">
			  <dt><p class="reference-name"><%= @admin_user.name %></p></dt>
			  <dd><%= @admin_user.username %></dd>
		</dl>
		<div class="form-actions">
  		<%= submit_tag ("Delete Admin"), :class => 'btn btn-danger' %>
  		<%= link_to("Cancel", {:action => 'index'}, :class => 'back-link')%>
    	</div>
	<% end %>
</div>
